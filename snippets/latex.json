"\\small{Rohan Kumar, \\texttt{21MS019}\\hfill\\today}",
"\\vspace{20pt}",
"",
"\t $4",
"",
"\\end{document}"]
},


//Footnotes

"footnote":{
	"prefix": "fnote",
	"body": "\\footnote{$1}"
},

"table footnote":{
	"prefix": "fnote",
	"body": "\\tablefootnote{$1}"
},

"footnotemark":{
	"prefix": "fmark",
	"body": "\\footnotemark"
},

"footnote text":{
	"prefix": "ftext",
	"body": "\\footnotetext{$1}"
},

//table

"table":{
	"prefix": "tab",	
	"body": [
"\\begin{table}[h]",
"\t \\centering",
"\t \\caption{${1:Caption}}",
"\t \\begin{tabular}{${2:rr}}",
"\t \\hline",
"\t \t $3 & $4 ${5:insert backslash}",
"\t \t \\hline",
"\t \t $6 & $7  $5",
"\t \t \\hline",
"\t \\end{tabular}",
"\\end{table}"]
},


// equations

"fraction":{
	"prefix":"frac",
	"body": "\\frac{$1}{$2}"
},

"brackets":{
	"prefix": "(",
	"body": "\\left($1\\right)"
},

//misc

"hyperlink":{
	"prefix": "href",
	"body": "\\href{${1:link}}{\\color{blue}${2:name}}"
},

"emphasized":{
	"prefix":"emph",
	"body": "\\emph{$1}"
},

"text bold":{
	"prefix": "bf",
	"body": "\\textbf{$1}"
},

"unit":{
	"prefix": "un",
	"body": "\\text{ $1}"
},

"caligraphic":{
	"prefix": "cal",
	"body": "\\textcal{$1}"
},

	
}