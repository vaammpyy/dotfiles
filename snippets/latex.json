{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

//begin

"begin":{
	"prefix": "beg",
	"body": [
		"\\begin{$1}",
	"\t $2",
"\\end{$1}"]
},

//Figure 
	"Figure":{
	"prefix": "fig",
	"body":[
	"\\begin{figure}[H]",
	"\t \\centering",
	"\t \\includegraphics[scale=${2:scale}]{${1:name}}",
	"\t \\caption{${3:caption}}",
	"\t \\label{fig:${label}}",
	"\\end{figure}",
]
	},

	//Equations

	"equation":{
		"prefix": "eq",
		"body": [
			"\\begin{equation*}",
		"\t ${1:insert equation here}",
	"\\end{equation*}"]
	},

	"equation_num":{
		"prefix": "eq",
		"body": [
			"\\begin{equation}",
		"\t ${1:insert equation here}",
	"\\end{equation}"]
	},

	"equation_boxed":{
		"prefix": "eq",
	"body": [
		"\\begin{equation*}",
	"\t \\boxed{${1:insert here}},",
"\\end{equation*}"]
	},

	"inline equation":{
		"prefix": "eq",
		"body": [
			"$${1:insert here}$"]
	},

	//Section types

	"abstract":{
		"prefix": "abs",
		"body": ["\\begin{abstract}",
		"$1",
	"\\end{abstract}"]
	},

	"section":{
		"prefix": "sec",
		"body": "\\section{${1:name}}"
	},

	"subsection":{
		"prefix": "sec",
		"body": "\\subsection{${1:name}}"
	},

	"subsubsection":{
		"prefix": "sec",
		"body": "\\subsubsection{${1:name}}"
	},

	//bullet points

	"item":{
		"prefix": "item",
		"body": [
			"\\begin{itemize}",
		"\t \\item $1",
	"\\end{itemize}"]
		},

	// project format

	"project":{
		"prefix": "proj",
		"body": [
			"\\documentclass{article}",

"\\usepackage[justification=centering]{caption}",
"\\usepackage[T1]{fontenc}",
"\\usepackage{geometry}",
"\\usepackage{tablefootnote}",
"\\usepackage{lipsum}",
"\\usepackage{makecell}",
"\\usepackage{booktabs}",
"\\usepackage{amsmath, amssymb, amsthm}",
"\\usepackage{listings}",
"\\usepackage{xcolor}",
"\\usepackage{hyperref}",
"\\usepackage{graphicx}",
"\\usepackage{fancyhdr}",
"\\usepackage{lipsum}",
"\\usepackage{longtable}",
"\\usepackage{caption}",
"\\usepackage{subcaption}",
"\\usepackage{float}",
"",
"\\begin{document}",
"\\begin{titlepage}",
	"\t \\centering",
	"\t \\includegraphics[scale=${2:scale}]{${1:image}}\\par\\vspace{1cm}",
	"\t \\vspace{1cm}",
	"\t {\\scshape \\Huge ${3:course name}\\par}",
	"\t \\vspace{2cm}",
	"\t {\\Huge\\bfseries ${4:Title}\\par}",
	"\t \\vspace{2cm}",
    "\t {\\Large{Rohan Kumar} ${5:insert double backslash} \\large{21MS019} $5 \\tt rk21ms019@iiserkol.ac.in\\par}",
	"\t \\vspace{2cm}",
	"\t \\vfill",
"% Bottom of the page",
	"\t {\\large \\today\\par}",
"\\end{titlepage}",
"",
"\\tableofcontents",
"",
"\\newpage",
"",
"\\section{$6}",
"$7",
"",
"\\end{document}",
]
	},

